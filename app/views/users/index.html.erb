<div class="container">
  <div class="row">
  <h1>ユーザー一覧</h1>
    <% @users.each do |user| %>
        <div class="container-child col-md-4 col-sm-4 col-xs-12 ">
          <div class="well well-sm user name">
            <%= profile_img(user) if profile_img(user) %>
            <%= user.name %></ br>
            <!--%= render 'follow_form', user: user %-->
            <%= render 'user', user: user %>
          </div>
        </div>
    <% end %>
  </div>
</div>
